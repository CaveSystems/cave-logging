<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <!--
            netstandard1.6;
            netstandard2.0;netstandard2.1;
            net5.0;net6.0;net7.0;
            netcoreapp3.0;netcoreapp3.1;
            netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;

        -->
        <TargetFrameworks>
            net20;net35;
            net40;
            net45;net451;net452;
            net46;net461;net462;
            net47;net471;net472;
            net48;
        </TargetFrameworks>
        <!--project-->
        <RootNamespace>Cave.Logging</RootNamespace>
        <AssemblyOriginatorKeyFile>Cave.Logging.snk</AssemblyOriginatorKeyFile>
        <Product>CaveSystems Logging Library</Product>
        <PackageTags>CaveSystems Logging</PackageTags>
        <Description>CaveSystems Logging Bibliothek.</Description>
        <PackageProjectUrl>https://github.com/CaveSystems/cave-logging</PackageProjectUrl>
        <RepositoryUrl>https://github.com/CaveSystems/cave-logging</RepositoryUrl>
        <!--settings-->
        <PackageReleaseNotes />
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <SignAssembly>true</SignAssembly>
        <NeutralLanguage />
        <DelaySign>false</DelaySign>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Cave.Backports" Version="3.0.4" />
        <PackageReference Include="Cave.Extensions" Version="3.0.2" />
        <PackageReference Include="Cave.IO" Version="3.0.2" />
        <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.3">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

</Project>